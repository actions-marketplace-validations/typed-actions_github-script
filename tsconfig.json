{
  "compilerOptions": {
    "lib": [
      "es2018",
    ],
    "module": "Node16",
    "moduleResolution": "Node16",
    "target": "es2018",
    "noEmit": true,
    "strict": true,
    "skipLibCheck": false,
    "forceConsistentCasingInFileNames": true,
    "allowUnusedLabels": false,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "allowJs": true,
    "checkJs": true,
  },
  "include": [
    "actions/*.mjs",
    "global.d.ts"
  ]
}